{
	"conf": {
		"version": "1.0",
		"type": "operator",
		"operator": {
			"name": "iat",
			"cmd": "iat.py",
			"type": "python",
			"version": "1.0",
			"description": "用户时序行为分析及异常检测",
			"category": "时间序列挖掘",
			"developer": "中科院计算所",
			"input": [
				{
					"name": "input",
					"format": "csv",
					"description": "输入文件路径, 文件格式不限于下面(默认)顺序和时间格式",
					"table": [
						{
							"name": "时间",
							"type": "string",
							"min": "",
							"max": "",
							"description": "格式为示例数据%Y-%m-%d %H:%M:%S"
						},
						{
							"name": "用户ID",
							"type": "int",
							"min": "0",
							"max": "",
							"description": "从0开始的整数"
						},
						{
							"name": "消息ID",
							"type": "int",
							"min": "",
							"max": "",
							"description": "长度为16位不连续数字"
						}
					]
				}
			],
			"argument": [
				{
					"name": "delimeter",
					"type": "string",
					"required": "false",
					"default": ",",
					"description": "输入文件分割符;示例1: , 示例2: \\x01 "
				},
				{
					"name": "groupids",
					"type": "list",
					"required": "false",
					"default": "1",
					"description": "按照groupid(s)聚合时间序列,id从0开始);示例数据：1 (表示第二列)"
				},
				{
					"name": "timeidx",
					"type": "int",
					"required": "false",
					"default": "0",
					"description": "时间列的id(id从0开始);示例数据：0 (表示第一列)"
				},
				{
					"name": "timeformat",
					"type": "string",
					"required": "false",
					"default": "%Y-%m-%d %H:%M:%S",
					"description": "输入数据的时间格式;示例数据%Y-%m-%d %H:%M:%S"
				},
				{
					"name": "gridsize",
					"type": "int",
					"required": "false",
					"default": 100,
					"description": "Histogram的网格数，示例格式:100"
				},
				{
					"name": "xlabel",
					"type": "string",
					"required": "false",
					"default": "IATn",
					"description": "Histogram横坐标的标签"
				},
				{
					"name": "ylabel",
					"type": "string",
					"required": "false",
					"default": "IATn+1",
					"description": "Histogram纵坐标的标签"
				}
			],
			"output": [
				{
					"name": "output",
					"format": ".jpg",
					"description": "输出图片"
				}
			],
			"requirements": {
				"type": "string",
				"default": "./requirements.txt"
			}
		}
	}
}
